<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="40dp"
    android:height="40dp"
    android:viewportHeight="140"
    android:viewportWidth="140">

    <group
        android:name="rotating_arrow"
        android:pivotX="70"
        android:pivotY="70">
        <path
            android:name="circle"
            android:pathData="M 70 20 A 50 50 0 1 1 20 70"
            android:strokeColor="@color/restart_arrow_color"
            android:strokeWidth="8" />

        <path
            android:name="triangle"
            android:fillColor="@color/restart_arrow_color"
            android:pathData="M 0 75 L 40 75 L 20 50 Z"
            android:strokeColor="@color/restart_arrow_color" />
    </group>
</vector>